{
  "name": "rng",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "lint:all": "nx workspace-lint && nx run-many --all --target=lint",
    "test:all": "nx run-many --all --target=test --browsers ChromeHeadless",
    "build:all": "nx run-many --all --target=build",
    "prepare": "nx clear-cache && npm run lint:all && npm run test:all && npm run build:all",
    "add-auth-module": "npm run workspace-schematic -- auth-module module-name --project=rng-project",
    "main_build_github": "nx build --project rng-app --output-path docs --base-href https://rime-dev.github.io/rng/",
    "generate_story_for_project": "nx g @nrwl/angular:storybook-configuration project-name",
    "run_story_for_project": "nx run project-name:storybook",
    "run_story_for_ui": "nx run ui:storybook",
    "run_story_for_util": "nx run util:storybook",
    "generate_tailwind": "nx generate @ngneat/tailwind:nx-setup",
    "add_material": "ng add @angular/material",
    "generate_app": "npx nx g @nrwl/angular:app map-app",
    "generate_directive": "npx nx g directive lazy-load-image/lazy-load-image --project=util --export"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.0",
    "@angular/cdk": "^12.1.0",
    "@angular/common": "^12.1.0",
    "@angular/compiler": "^12.1.0",
    "@angular/core": "^12.1.0",
    "@angular/forms": "^12.1.0",
    "@angular/material": "^12.1.0",
    "@angular/platform-browser": "^12.1.0",
    "@angular/platform-browser-dynamic": "^12.1.0",
    "@angular/router": "^12.1.0",
    "@nrwl/angular": "12.6.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "^12.1.0",
    "@angular/language-service": "^12.1.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@ngneat/tailwind": "^7.0.3",
    "@nrwl/cli": "12.6.6",
    "@nrwl/cypress": "12.6.6",
    "@nrwl/devkit": "12.6.6",
    "@nrwl/eslint-plugin-nx": "12.6.6",
    "@nrwl/jest": "12.6.6",
    "@nrwl/linter": "12.6.6",
    "@nrwl/storybook": "^12.7.1",
    "@nrwl/tao": "12.6.6",
    "@nrwl/workspace": "12.6.6",
    "@storybook/addon-essentials": "~6.3.0",
    "@storybook/angular": "~6.3.0",
    "@storybook/builder-webpack5": "~6.3.0",
    "@storybook/manager-webpack5": "~6.3.0",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "autoprefixer": "^10.3.1",
    "cypress": "^7.3.0",
    "dotenv": "~10.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.2.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "4.3.8",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.4",
    "ng-packagr": "12.2.1",
    "ol": "^6.7.0",
    "postcss": "^8.3.6",
    "prettier": "^2.3.1",
    "tailwindcss": "^2.2.7",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "~4.3.5"
  }
}
