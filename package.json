{
  "name": "ngx-rime",
  "version": "0.2.0",
  "description": "Angular architecture utilities",
  "keywords": [
    "Angular"
  ],
  "homepage": "https://github.com/rime-dev/ngx-rime",
  "author": "Carlos Bayarri Cebrecos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rime-dev/ngx-rime.git"
  },
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "lint:all": "nx workspace-lint && nx run-many --projects=data-access,feature,ui,util --target=lint",
    "test:all": "nx run-many --projects=data-access,feature,ui,util  --target=test --browsers ChromeHeadless",
    "build:all": "nx clear-cache && nx run-many --projects=data-access,feature,ui,util  --target=build",
    "check:ui": "nx lint ui && nx test ui --browsers ChromeHeadless && nx build ui",
    "check:util": "nx lint util && nx test util --browsers ChromeHeadless && nx build util",
    "check:feature": "nx lint feature && nx test feature --browsers ChromeHeadless && nx build feature",
    "check:data-access": "nx lint data-access && nx test data-access --browsers ChromeHeadless && nx build data-access",
    "check": "nx clear-cache && npm run lint:all && npm run test:all && npm run build:all",
    "add-auth-module": "npm run workspace-schematic -- auth-module module-name --project=ngx-rime-project",
    "main_build_github": "nx build --project ngx-rime-app --output-path docs --base-href https://rime-dev.github.io/ngx-rime/",
    "stories:data-access": "nx g @nx/angular:stories data-access",
    "stories:ui": "nx g @nx/angular:stories ui",
    "run_story_for_project": "nx run project-name:storybook",
    "run_story_for_ui": "nx run ui:storybook",
    "run_story_for_util": "nx run util:storybook",
    "generate_tailwind": "nx generate @ngneat/tailwind:nx-setup",
    "generate_stotybook_config": "nx generate @nx/angular:storybook-configuration ui",
    "add_material": "ng add @angular/material",
    "generate_directive": "npx nx g directive lazy-load-image/lazy-load-image --project=util --export",
    "npm_only_pack_util": "cd dist/libs/util && npm pack",
    "npm_only_pack_ui": "cd dist/libs/ui && npm pack",
    "npm_only_pack_feature": "cd dist/libs/feature && npm pack",
    "npm_only_pack_data_access": "cd dist/libs/data-access && npm pack",
    "npm_only_build_pack_data_access": "nx build data-access && cd dist/libs/data-access && npm version 0.2.1 && npm pack",
    "npm_only_pack_libs": "npm run npm_only_pack_util && npm run npm_only_pack_ui && npm run npm_only_pack_feature && npm run npm_only_pack_data_access",
    "build&pack": "npm run build:all && npm run npm_only_pack_libs",
    "npm_publish_util": "cd dist/libs/util && npm version 0.2.1 && npm pack && npm publish",
    "npm_publish_ui": "cd dist/libs/ui && npm version 0.2.1 && npm pack && npm publish",
    "npm_publish_feature": "cd dist/libs/feature && npm version 0.2.1 && npm pack && npm publish",
    "npm_publish_data_access": "cd dist/libs/data-access && npm version 0.2.1 && npm pack && npm publish",
    "publish:libs": "nx workspace-generator publish",
    "remove_angular_cache": "rm -R .angular"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "16.1.8",
    "@angular/cdk": "16.1.7",
    "@angular/common": "16.1.8",
    "@angular/compiler": "16.1.8",
    "@angular/core": "16.1.8",
    "@angular/fire": "^7.2.0",
    "@angular/forms": "16.1.8",
    "@angular/material": "16.1.7",
    "@angular/platform-browser": "16.1.8",
    "@angular/platform-browser-dynamic": "16.1.8",
    "@angular/router": "16.1.8",
    "@ngneat/transloco": "3.1.3",
    "@ngneat/transloco-locale": "^3.0.1",
    "@ngrx/data": "16.0.1",
    "@ngrx/effects": "16.0.1",
    "@ngrx/entity": "16.0.1",
    "@ngrx/store": "16.0.1",
    "@ngrx/store-devtools": "16.0.1",
    "@storybook/core-server": "7.2.1",
    "@swc/helpers": "~0.5.0",
    "d3": "7.3.0",
    "firebase": "^9.4.0",
    "ol": "6.12.0",
    "rxfire": "^6.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.7",
    "@angular-devkit/core": "16.1.7",
    "@angular-devkit/schematics": "16.1.7",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "16.1.8",
    "@angular/language-service": "16.1.8",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@ngneat/tailwind": "^7.0.3",
    "@nx/cypress": "16.6.0",
    "@nx/devkit": "16.6.0",
    "@nx/eslint-plugin": "16.6.0",
    "@nx/jest": "16.6.0",
    "@nx/js": "16.6.0",
    "@nx/linter": "16.6.0",
    "@nx/node": "16.6.0",
    "@nx/plugin": "16.6.0",
    "@nx/storybook": "16.6.0",
    "@nx/workspace": "16.6.0",
    "@schematics/angular": "16.1.7",
    "@storybook/addon-essentials": "7.2.1",
    "@storybook/angular": "7.2.1",
    "@storybook/builder-webpack5": "7.2.1",
    "@storybook/manager-webpack5": "6.5.16",
    "@swc-node/register": "~1.4.2",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.51",
    "@types/d3": "7.0.0",
    "@types/jest": "29.4.4",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "autoprefixer": "^10.3.1",
    "cypress": "^7.3.0",
    "dotenv": "~10.0.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "4.3.8",
    "jest": "29.4.3",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "13.1.1",
