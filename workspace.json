            "outputPath": "dist/apps/rng-app",
            "index": "apps/rng-app/src/index.html",
            "main": "apps/rng-app/src/main.ts",
            "polyfills": "apps/rng-app/src/polyfills.ts",
            "tsConfig": "apps/rng-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/rng-app/src/favicon.ico", "apps/rng-app/src/assets"],
            "styles": ["apps/rng-app/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/rng-app/src/environments/environment.ts",
                  "with": "apps/rng-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "rng-app:build:production"
            },
            "development": {
              "browserTarget": "rng-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "rng-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/rng-app/src/**/*.ts", "apps/rng-app/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/rng-app"],
          "options": {
            "jestConfig": "apps/rng-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "rng-app-e2e": {
      "root": "apps/rng-app-e2e",
      "sourceRoot": "apps/rng-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/rng-app-e2e/cypress.json",
            "tsConfig": "apps/rng-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "rng-app:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "rng-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/rng-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui",
      "prefix": "rng",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/ui"],
          "options": {
            "project": "libs/ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
